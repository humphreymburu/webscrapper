@model WordDictionary.Models.Dictionary
@{
    ViewData["Title"] = "Home Page";
}




 <div class="row justify-content-center text-center align-items-center">
    <div class="col">

        <form asp-controller="Home" asp-action="getResults" method="post">

            <div class="col-12 col-lg-5">
                <div class="form-group mb-lg-0">
                    <div class="input-group input-group-lg mb-lg-0">
                        <div class="input-group-prepend"></div>
                        <input asp-for="Url" class="form-control autocomplete" /> <br />
                    </div>
                </div>
            </div>

            <br />
            <br />

            <div class="col-12 col-lg-3">
                <button class="btn btn-lg btn-primary btn-block mt-3 mt-md-0 animate-up-2" type="submit">Enter Url</button>
            </div>
        </form>
    </div>
</div>



